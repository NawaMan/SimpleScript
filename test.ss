/** This is an exmaple code */

// Import area
import java.io.*;
import java.util.*;
import net.nawaman.script.*;

// Here is a local elements
static  int I = 0;
private int Factorial(int I) {
	return (I <= 1) ? 1 : I * Factorial(I - 1);
}

// Here is the Annotation
@SuppressWarnings("unchecked");

System.out.println("Hello World!!!");
System.out.println("Main Args = " + Arrays.toString($Args));	// Access Args
System.out.println("I = " + I);
System.out.println("Working Dir: " + (new File(".")).getAbsolutePath());

System.out.println("Factorial using local method");
for(int i = 0; i < 10; i++)
	System.out.println(i+"! = " + Factorial(i));

Function Fact = (Function)ScriptManager.Use("factorial");

System.out.println();
System.out.println("Factorial using ./ss_libs/factorial.ss via ScriptManager.Use(...)");
if(Fact != null) {
	for(int i = 0; i < 10; i++)
		System.out.println(i+"! = " + Fact.run(i));
}

System.out.println("DONE!!!");

return;



